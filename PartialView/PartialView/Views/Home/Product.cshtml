@model IEnumerable<PartialView.Models.Product>
@{
    ViewData["Title"] = "Product";
}

<h1>Product</h1>

<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4">
				<partial name="_ProductPartialView" model="item" />
            </div>
        }  
    </div>
</div>